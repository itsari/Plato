<h1>Edit Plate</h1>
<form
  action="/edit-plate/<%= plate.id %>"
  method="POST"
  enctype="multipart/form-data"
>
  <label for="plate">License Plate:</label>
  <input
    type="text"
    id="plate"
    name="plate"
    value="<%= plate.plate %>"
    required
  />

  <label for="owner">Owner:</label>
  <input type="text" id="owner" name="owner" value="<%= plate.owner %>" />

  <label for="phone">Phone:</label>
  <input type="text" id="phone" name="phone" value="<%= plate.phone %>" />

  <label for="vehicle_type">Vehicle Type:</label>
  <input
    type="text"
    id="vehicle_type"
    name="vehicle_type"
    value="<%= plate.vehicle_type %>"
  />

  <label for="color">Color:</label>
  <input type="text" id="color" name="color" value="<%= plate.color %>" />

  <label for="photos">Photos (up to 3):</label>
  <input type="file" id="photos" name="photos" multiple />

  <button type="submit">Update Plate</button>
</form>
