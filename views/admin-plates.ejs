<h1>Admin Dashboard</h1>
<p>Welcome, Admin!</p>
<% if (plates.length > 0) { %>
<ul>
  <% plates.forEach(plate => { %> <% plates.forEach(plate => { %>
  <li>
    <strong>Plate:</strong> <%= plate.plate %><br />
    <strong>Owner:</strong> <%= plate.owner || 'N/A' %><br />
    <strong>Phone:</strong> <%= plate.phone || 'N/A' %><br />
    <strong>Vehicle Type:</strong> <%= plate.vehicle_type || 'N/A' %><br />
    <strong>Color:</strong> <%= plate.color || 'N/A' %><br />
    <strong>Added By User ID:</strong> <%= plate.user_id %><br />
    <a href="/edit-plate/<%= plate.id %>">Edit</a> |
    <a
      href="/delete-plate/<%= plate.id %>"
      onclick="return confirm('Are you sure you want to delete this plate?')"
      >Delete</a
    >
  </li>
  <% }); %>
</ul>
<% } else { %>
<p>No plates found in the system.</p>
<% } %>
